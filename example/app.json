{
  "expo": {
    "scheme": "myapp",
    "plugins": [
      [
        "./plugins/expo-foreground-actions"
      ]
    ],
    "name": "expo-foreground-actions-example",
    "slug": "expo-foreground-actions-example",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "expo.modules.foregroundactions.example"
    },
    "android": {
      "permissions": [
        "FOREGROUND_SERVICE",
        "WAKE_LOCK",
        "POST_NOTIFICATIONS"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "expo.modules.foregroundactions.example"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
